{
  "name": "rdvue",
  "version": "0.5.4",
  "description": "command line tool for generating and managing vue projects",
  "author": "realdecoy",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "homepage": "https://github.com/realdecoy/rdvue#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realdecoy/rdvue.git"
  },
  "bin": {
    "rdvue": "./build/main/index.js"
  },
  "keywords": [],
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "lint": "tslint --fix --project tsconfig.json 'src/**/*.ts'",
    "pipeline_lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "clean": "trash build test"
  },
  "scripts-info": {
    "build": "Clean and rebuild the project",
    "test": "Lint and unit test the project"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.2",
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.0",
    "@types/clui": "^0.3.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/configstore": "^4.0.0",
    "@types/figlet": "^1.2.0",
    "@types/git-user-name": "^2.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.149",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/rimraf": "^2.0.3",
    "arg": "^4.1.0",
    "bluebird": "^3.5.5",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "command-line-usage": "^6.0.2",
    "configstore": "^5.0.0",
    "figlet": "^1.2.3",
    "git-user-name": "^2.0.0",
    "inquirer": "^6.4.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.0",
    "sha.js": "^2.4.11",
    "simple-git": "^1.118.0",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.9.0",
    "@types/bluebird": "^3.5.27",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "ava": "1.0.0-beta.7",
    "babel-jest": "^25.1.0",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.1",
    "jest": "^25.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.2",
    "standard-version": "^7.1.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^25.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts"
    ]
  }
}
