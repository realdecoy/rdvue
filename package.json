{
  "name": "rdvue",
  "version": "2.2.4",
  "author": "realdecoy",
  "bin": {
    "rdvue": "./bin/run"
  },
  "bugs": "https://github.com/realdecoy/rdvue/issues",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@oclif/core": "^2.6.4",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.2",
    "bluebird": "^3.7.2",
    "chalk": "4.1.2",
    "fs": "0.0.1-security",
    "git-user-name": "^2.0.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "prompts": "^2.4.2",
    "replace-in-file": "^6.2.0",
    "shelljs": "^0.8.5",
    "util": "^0.12.5",
    "vuetify": "^3.1.11"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@oclif/test": "^2.3.10",
    "@types/bluebird": "^3.5.33",
    "@types/chai": "^4",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.16",
    "@types/prompts": "^2.4.3",
    "@types/shelljs": "^0.8.8",
    "@types/sinon": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-oclif": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^9",
    "nyc": "^14.1.1",
    "oclif": "^3",
    "shx": "^0.3.3",
    "sinon": "^11.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://realdecoy.github.io/rdvue/#/",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "oclif": {
    "bin": "rdvue",
    "dirname": "rdvue",
    "commands": "./dist/commands",
    "helpClass": "./dist/help.js",
    "plugins": [],
    "topicSeparator": " "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realdecoy/rdvue.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "audit": "npm audit --production",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "shx rm -rf dist && shx rm -f oclif.manifest.json && shx rm -f tsconfig.tsbuildinfo && yarn build && oclif manifest",
    "test": "nyc mocha \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
